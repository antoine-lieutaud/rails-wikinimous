<h1><%= @article.title %> </h1>
<p><%= @article.content %></p> 

<p> <%= link_to "Edit article", edit_article_path(@article) %> </p>
<p> <%= link_to "Back to article list", articles_path %> </p>

<%= link_to "Delete #{@article.title}",
            article_path(@article),
            method: :delete,
            data: { confirm: "Are you sure?" } %>

